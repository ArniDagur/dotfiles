ctrl + shift + alt - u : yabai -m window --focus stack.prev || yabai -m window --focus north
ctrl + shift + alt - e : yabai -m window --focus stack.next || yabai -m window --focus south
ctrl + shift + alt - a : yabai -m window --focus west
ctrl + shift + alt - o : yabai -m window --focus east

ctrl + shift + alt + cmd - u: yabai -m window --swap north
ctrl + shift + alt + cmd - e: yabai -m window --swap south
ctrl + shift + alt + cmd - a : yabai -m window --swap west
ctrl + shift + alt + cmd - o : yabai -m window --swap east

ctrl + shift + alt - 1 : yabai -m space --focus 1
ctrl + shift + alt - 2 : yabai -m space --focus 2
ctrl + shift + alt - 3 : yabai -m space --focus 3
ctrl + shift + alt - 4 : yabai -m space --focus 4
ctrl + shift + alt - 5 : yabai -m space --focus 5
ctrl + shift + alt - 6 : yabai -m space --focus 6
ctrl + shift + alt - b : yabai -m space --focus next
ctrl + shift + alt - f : yabai -m space --focus prev

ctrl + shift + alt + cmd - 1 : yabai -m window --space 1
ctrl + shift + alt + cmd - 2 : yabai -m window --space 2
ctrl + shift + alt + cmd - 3 : yabai -m window --space 3
ctrl + shift + alt + cmd - 4 : yabai -m window --space 4
ctrl + shift + alt + cmd - 5  : yabai -m window --space 5
ctrl + shift + alt + cmd - 6 : yabai -m window --space 6

ctrl + shift + alt - x : yabai -m space --mirror y-axis
ctrl + shift + alt - y : yabai -m space --mirror x-axis
ctrl + shift + alt - r : yabai -m space --rotate 90
ctrl + shift + alt - j : yabai -m space --balance

ctrl + shift + alt - q : yabai -m window --close
ctrl + shift + alt - return : /Applications/kitty.app/Contents/MacOS/kitty --single-instance -d ~
